<style>
    .text-gradient {
        background: var(--gradient-primary);
        -webkit-background-clip: text;
        -webkit-text-fill-color: transparent;
        background-clip: text;
    }

    .hero-section {
        padding: 2rem 0;
    }

    .hero-content h1 {
        line-height: 1.2;
    }

    .hero-image {
        position: relative;
    }

    .workflow-animation {
        font-size: 8rem;
        color: var(--primary-color);
        opacity: 0.1;
        animation: float 3s ease-in-out infinite;
    }

    @@keyframes float {
        0%, 100% { transform: translateY(0px); }
        50% { transform: translateY(-20px); }
    }

    .quick-action-card {
        display: block;
        text-decoration: none;
        color: inherit;
        padding: 1.5rem;
        border-radius: 12px;
        background: rgba(255, 255, 255, 0.5);
        border: 1px solid rgba(255, 255, 255, 0.2);
        transition: all 0.3s ease;
        text-align: center;
    }

    .quick-action-card:hover {
        transform: translateY(-5px);
        box-shadow: var(--shadow-lg);
        color: inherit;
        text-decoration: none;
    }

    .quick-action-icon {
        width: 60px;
        height: 60px;
        border-radius: 50%;
        background: var(--gradient-primary);
        display: flex;
        align-items: center;
        justify-content: center;
        margin: 0 auto 1rem;
        font-size: 24px;
        color: white;
    }

    .quick-action-card h5 {
        margin-bottom: 0.5rem;
        font-weight: 600;
    }

    .quick-action-card p {
        color: var(--secondary-color);
        margin-bottom: 0;
        font-size: 0.9rem;
    }

    .activity-list {
        space-y: 1rem;
    }

    .activity-item {
        display: flex;
        align-items: flex-start;
        padding: 1rem 0;
        border-bottom: 1px solid rgba(0,0,0,0.05);
    }

    .activity-item:last-child {
        border-bottom: none;
    }

    .activity-icon {
        width: 40px;
        height: 40px;
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        margin-left: 1rem;
        flex-shrink: 0;
        color: white;
        font-size: 14px;
    }

    .activity-content h6 {
        margin-bottom: 0.25rem;
        font-weight: 600;
    }

    .mini-stats {
        space-y: 1rem;
    }

    .mini-stat-item {
        display: flex;
        align-items: center;
        padding: 0.75rem 0;
    }

    .mini-stat-icon {
        width: 40px;
        height: 40px;
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        margin-left: 1rem;
        color: white;
        font-size: 16px;
    }

    .mini-stat-content {
        display: flex;
        flex-direction: column;
    }

    .mini-stat-number {
        font-size: 1.5rem;
        font-weight: 700;
        line-height: 1;
    }

    .mini-stat-label {
        font-size: 0.875rem;
        color: var(--secondary-color);
    }

    @@media (max-width: 768px) {
        .hero-content h1 {
            font-size: 2rem;
        }
        
        .workflow-animation {
            font-size: 4rem;
        }
        
        .hero-buttons {
            text-align: center;
        }
        
        .hero-buttons .btn {
            display: block;
            width: 100%;
            margin-bottom: 1rem;
        }
    }
</style>



